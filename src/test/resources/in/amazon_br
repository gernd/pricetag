<script type="text/javascript">
 
P.when('DynamicIframe').execute(function(DynamicIframe){
    var BookDescriptionIframe = null,
        bookDescEncodedData = "%22Harari%20%26%23xE9%3B%20brilhante%20%5B...%5D%20Sapiens%20%26%23xE9%3B%20realmente%20impressionante%2C%20de%20se%20ler%20num%20f%26%23xF4%3Blego%20s%26%23xF3%3B.%20De%20fato%2C%20questiona%20nossas%20ideias%20preconcebidas%20a%20respeito%20do%20universo.%22%20%28The%20Guardian%29%20%26%23x201C%3B...uma%20explos%26%23xE3%3Bo%20em%20forma%20de%20livro%2C%20t%26%23xE3%3Bo%20agrad%26%23xE1%3Bvel%20de%20ler%20quanto%20perturbador.%26%23x201D%3B%20The%20Sunday%20Times%20%26%23x201C%3BIlumina%20as%20grandes%20quest%26%23xF5%3Bes%20da%20hist%26%23xF3%3Bria%20e%20do%20mundo%20moderno.%26%23x201D%3B%20Jared%20Diamond%2C%20autor%20de%20Armas%2C%20germes%20e%20a%26%23xE7%3Bo%20%26%23x201C%3BEste%20livro%20fascinante%20n%26%23xE3%3Bo%20pode%20ser%20resumido%3B%20voc%26%23xEA%3B%20simplesmente%20ter%26%23xE1%3B%20de%20l%26%23xEA%3B-lo.%26%23x201D%3B%20Financial%20Times%20%26%23x201C%3BHarari%20sabe%20escrever%20%5B...%5D%20de%20verdade%2C%20com%20gosto%2C%20clareza%2C%20eleg%26%23xE2%3Bncia%20e%20um%20olhar%20cl%26%23xED%3Bnico%20para%20a%20met%26%23xE1%3Bfora.%26%23x201D%3B%20The%20Times%20Um%20relato%20eletrizante%20sobre%20a%20aventura%20de%20nossa%20extraordin%26%23xE1%3Bria%20esp%26%23xE9%3Bcie%20%26%23x2013%3B%20de%20primatas%20insignificantes%20a%20senhores%20do%20mundo.%20O%20que%20possibilitou%20ao%20Homo%20sapiens%20subjugar%20as%20demais%20esp%26%23xE9%3Bcies%3F%20O%20que%20nos%20torna%20capazes%20das%20mais%20belas%20obras%20de%20arte%2C%20dos%20avan%26%23xE7%3Bos%20cient%26%23xED%3Bficos%20mais%20impens%26%23xE1%3Bveis%20e%20das%20mais%20horripilantes%20guerras%3F%20Yuval%20Noah%20Harari%20aborda%20de%20forma%20brilhante%20estas%20e%20muitas%20outras%20quest%26%23xF5%3Bes%20da%20nossa%20evolu%26%23xE7%3B%26%23xE3%3Bo.%20Ele%20repassa%20a%20hist%26%23xF3%3Bria%20da%20humanidade%2C%20relacionando%20com%20quest%26%23xF5%3Bes%20do%20presente.%20E%20consegue%20isso%20de%20maneira%20surpreendente.%20Doutor%20em%20hist%26%23xF3%3Bria%20pela%20Universidade%20de%20Oxford%20e%20professor%20do%20departamento%20de%20Hist%26%23xF3%3Bria%20da%20Universidade%20Hebraica%20de%20Jerusal%26%23xE9%3Bm%2C%20seu%20livro%20n%26%23xE3%3Bo%20entrou%20por%20acaso%20nas%20listas%20dos%20mais%20vendidos%20de%2040%20pa%26%23xED%3Bses%20para%20os%20quais%20foi%20traduzido.%20Sapiens%20impressiona%20pela%20quantidade%20de%20informa%26%23xE7%3B%26%23xE3%3Bo%2C%20oferecida%20em%20linguagem%20acess%26%23xED%3Bvel%2C%20atraente%20e%20espirituosa.%20Tanto%20que%2C%20na%20primeira%20semana%20de%20lan%26%23xE7%3Bamento%20nos%20Estados%20Unidos%2C%20j%26%23xE1%3B%20figurava%20entre%20os%20mais%20vendidos%20na%20lista%20do%20The%20New%20York%20Times.%20Em%20Sapiens%2C%20Harari%20nos%20oferece%20n%26%23xE3%3Bo%20apenas%20conhecimento%20evolutivo%2C%20mas%20tamb%26%23xE9%3Bm%20sociol%26%23xF3%3Bgico%2C%20antropol%26%23xF3%3Bgico%20e%20at%26%23xE9%3B%20mesmo%20econ%26%23xF4%3Bmico.%20Ele%20se%20baseia%20nas%20mais%20recentes%20descobertas%20de%20diferentes%20campos%20como%20paleontologia%2C%20biologia%20e%20antropologia.%20E%2C%20especialmente%20para%20a%20edi%26%23xE7%3B%26%23xE3%3Bo%20brasileira%2C%20realizou%20algumas%20atualiza%26%23xE7%3B%26%23xF5%3Bes%20no%20final%20de%202014.%20Esta%20edi%26%23xE7%3B%26%23xE3%3Bo%20traz%20dezenas%20de%20imagens%2C%20mapas%20e%20tabelas%20que%20o%20deixam%20ainda%20mais%20din%26%23xE2%3Bmico.",
        bookDescriptionAvailableHeight,
        minBookDescriptionInitialHeight = 112,
        options = {},
        iframeId = "bookDesc_iframe";
 
    function resizeCallback() {
        P.guardFatal("bookDescription", function() {
            // Get the line-height of the iframe
            var iframe = document.getElementById(iframeId);
            var iframeDocument = iframe.contentDocument;

            if (false && iframeDocument && iframeDocument.defaultView) {
                // Set the height to the number of lines specified
                var numLines = parseInt(0, 10);
 
                // Get the line-height of the iframe
                var iframeContent = iframeDocument.getElementById("iframeContent");
 
                // Compute the line height
                var lineHeight = iframeDocument.defaultView.getComputedStyle(iframeContent, null).getPropertyValue("line-height");

                // Parse the line height
                lineHeight = parseFloat(lineHeight);
                bookDescriptionAvailableHeight = Math.round(lineHeight * numLines);
            } else {
                var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
                var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
                var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
                bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
                if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
                    bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
                }
            }
            
            var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;
 
            if(psTotalHeight > bookDescriptionAvailableHeight + 30){
                if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
                    document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
                    document.getElementById("psPlaceHolder").style.display ="block";
                    document.getElementById("bdSeeAllPrompt").style.display ="block";
                }
                else{
                    document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
                }
            }
            else{
                document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
                document.getElementById("psPlaceHolder").style.display ="none";
                document.getElementById("bdSeeAllPrompt").style.display ="block";
                document.getElementById("bdSeeLessPrompt").style.display ="none";
                document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
            }
        })();
    }

    options.iframeId = iframeId;
    options.iframeWrapperId = "bookDesc_iframe_wrapper";
    options.overriddenCSSId = "bookDesc_override_CSS";
    options.encodedIframeContent = bookDescEncodedData;
    options.initialResizeCallback = resizeCallback;
    
    BookDescriptionIframe = new DynamicIframe(options);
    P.guardFatal("bookDescription", function() {
        BookDescriptionIframe.createIframe();
    }) ();
        
    if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
      P.when('jQuery').execute(function($) {
        $(window).resize(function() {
            P.guardFatal("bookDescription", function() {
                BookDescriptionIframe.resizeIframe(resizeCallback);
            }) ();
        });

        $(window).bind('imageResize', function() {
            P.guardFatal("bookDescription", function() {
                BookDescriptionIframe.resizeIframe(resizeCallback);
            }) ();
        });
      });
    }
});
</script>


                    
                
            
        
        
        
        
        
        
    
    
    

    















