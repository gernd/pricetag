<script type="text/javascript">

function prefetchTYPAssets() {
    var imageAssets = new Array();
    var jsCssAssets = new Array();
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/32/x-locale/common/buy-buttons/review-1-click-order._CB355874991_.gif");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/32/x-locale/common/buttons/continue-shopping._CB391867842_.gif");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/32/x-locale/common/buy-buttons/thank-you-elbow._CB394098248_.gif");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/32/x-locale/communities/social/snwicons_v2._CB387445848_.png");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/32/checkout/assets/carrot._CB394097702_.gif");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/32/gno/sprites/nav-sprite-global_bluebeacon-1x_optimized._CB515850632_.png");
      imageAssets.push("https://images-na.ssl-images-amazon.com/images/G/32/x-locale/common/transparent-pixel._CB386942704_.gif");

    // pre-fetching image assets
    for (var i=0; i<imageAssets.length; i++) {
       new Image().src = imageAssets[i];
    }
    // pre-fetching css and js assets based on different browser types
    var isIE = /*@cc_on!@*/0;
    var isFireFox = /Firefox/.test(navigator.userAgent);
    if (isIE) {
      for (var i=0; i<jsCssAssets.length; i++) {
        new Image().src = jsCssAssets[i];
      }
    }
    else if (isFireFox) {
      for (var i=0; i<jsCssAssets.length; i++) {
        var o =  document.createElement("object");
        o.data = jsCssAssets[i];
        o.width = o.height = 0;
        document.body.appendChild(o);
      }
    }
}

    var onload = function () {
        setTimeout(prefetchTYPAssets, 2000);
    };
    if (window.addEventListener) {
        window.addEventListener("load", onload);
    } else if (window.attachEvent) { /* for <= IE 8 */
        window.attachEvent("onload", onload);
    }
</script>

