<script type="text/javascript">

function prefetchTYPAssets() {
    var imageAssets = new Array();
    var jsCssAssets = new Array();
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/x-locale/common/buy-buttons/review-1-click-order._CB192198599_.gif");
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/x-locale/common/buttons/continue-shopping._CB192188120_.gif");
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/x-locale/common/buy-buttons/thank-you-elbow._CB192198531_.gif");
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/x-locale/communities/social/snwicons_v2._CB402336182_.png");
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/checkout/assets/carrot._CB192253931_.gif");
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/checkout/thank-you-page/assets/yellow-rounded-corner-sprite._CB192253922_.gif");
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/checkout/thank-you-page/assets/white-rounded-corner-sprite._CB212531240_.gif");
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/gno/sprites/nav-sprite-global_bluebeacon-V3-1x_optimized._CB516557022_.png");
      imageAssets.push("https://images-eu.ssl-images-amazon.com/images/G/02/x-locale/common/transparent-pixel._CB384789948_.gif");

    // pre-fetching image assets
    for (var i=0; i<imageAssets.length; i++) {
       new Image().src = imageAssets[i];
    }
    // pre-fetching css and js assets based on different browser types
    var isIE = /*@cc_on!@*/0;
    var isFireFox = /Firefox/.test(navigator.userAgent);
    if (isIE) {
      for (var i=0; i<jsCssAssets.length; i++) {
        new Image().src = jsCssAssets[i];
      }
    }
    else if (isFireFox) {
      for (var i=0; i<jsCssAssets.length; i++) {
        var o =  document.createElement("object");
        o.data = jsCssAssets[i];
        o.width = o.height = 0;
        document.body.appendChild(o);
      }
    }
}

    var onload = function () {
        setTimeout(prefetchTYPAssets, 2000);
    };
    if (window.addEventListener) {
        window.addEventListener("load", onload);
    } else if (window.attachEvent) { /* for <= IE 8 */
        window.attachEvent("onload", onload);
    }
</script>

