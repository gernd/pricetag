    <script type="text/javascript">
        "use strict";
        
        /**
        * Specialized address dropdown that hides the traditional AUI dropdown
        * interface and empowers a substitute textual button to trigger the
        * dropdown instead.
        */
        P.when("A", "ready").execute(function(A){
            var $ = A.$;
            function registerAddressDropdownEvents() {
                // Detect clicks on the text button and redirect them to the AUI button
                // in as generic a fashion possible.
                $('#ubbAddressDropdownTextButton').click(function(e) {
                    e.preventDefault();
                    $('#shipToAUIButtonUBB').find('[data-action]="a-dropdown-button"')[0].click();
                    });

                // Update the text button when a new address is selected.
                A.on('a:dropdown:ubbShipTo:select', function(data) {
                    // Change the address dropdown button inner text to reflect the new address.
                    var newDropdownAddress = $('#' + data.auiItemNode.id).text();
                    $('#ubbAddressDropdownSelectedAddress').html(newDropdownAddress);
                });
            }
            registerAddressDropdownEvents();
        });
    </script>


                </div>
                
            </div>
            
            <div class="a-row">
                














    
    
    
        
        
        
    
    






