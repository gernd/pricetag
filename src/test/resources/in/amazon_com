<script type="text/javascript">
    P.when('A').execute(function (A){
        var $ = A.$;
        var data = {
            'landingState': 'new'
        };
        var actionPanelLandingStateData = A.state("actionPanelState");
        if(typeof actionPanelLandingStateData !== 'undefined' && typeof actionPanelLandingStateData['landingState'] !== 'undefined'){
            A.state.replace('actionPanelState', data);
        }else{
            A.state('actionPanelState', data);
        }
    });
   
    function replaceAll(str, find, replace) {
        return str.replace(new RegExp(find, 'g'), replace);
    }
    
    function clearRadioButtonClass(buttonType) {
        var currClassName = document.querySelector('.superleaf .selectionButtons #' + buttonType + 'RadioButton').className;
        currClassName = replaceAll(currClassName, buttonType + 'PriceDisabled', '');
        document.querySelector('.superleaf .selectionButtons #' + buttonType + 'RadioButton').className = currClassName;
    }
    
    if(!(window.superleaf && (window.superleaf.desiredRadioButtonState || window.superleaf.desiredDigitalSwatchIndex))){
        document.querySelector('.superleaf .selectionButtons').className += ' ';
        clearRadioButtonClass('digital');
        clearRadioButtonClass('new');
        clearRadioButtonClass('used');
    }
</script>
