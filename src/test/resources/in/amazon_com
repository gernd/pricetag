<script type="text/javascript">
    P.when('A').execute(function(A){
        var $ = A.$;
        var actionPanel = $("#superleafActionPanel");
        var actionPanelClasses = ['new', 'used', 'digital'];
        var actionPanelClassPrefix = 'ap-offer-selector-';
    
        function clearActionPanelState(){
            for (var i = 0; i < actionPanelClasses.length; i++) {
                actionPanel.removeClass(actionPanelClassPrefix + actionPanelClasses[i]);
            }
        }

        function setActionPanel(desiredState) {
            $('.superleaf .selectionButtons #' + desiredState + 'RadioButton input:radio:first').prop( "checked", true );
            clearActionPanelState();
            var className = actionPanelClassPrefix + desiredState;
            actionPanel.addClass(className);
        }
        
        A.declarative('buybox-offer-toggle', 'click', function(event){
            var desiredState = event.data['state'];
            setActionPanel(desiredState);
        });

        function actionPanelInit(){
            var actionPanelLandingStateData = A.state("actionPanelState");
            var apClass;
            if(typeof actionPanelLandingStateData !== 'undefined' && typeof actionPanelLandingStateData['landingState'] !== 'undefined'){
                apClass = actionPanelLandingStateData['landingState'];
            }
            else {
                apClass = 'new';
            }
            if(window.superleaf && window.superleaf.desiredRadioButtonState){
                apClass = window.superleaf.desiredRadioButtonState;
            } else if(!(window.superleaf && window.superleaf.desiredDigitalSwatchIndex)){
                $('.superleaf .selectionButtons').removeClass('singleRadioButton');
                $('.superleaf .selectionButtons').removeClass('noRadioButton');
                if(window.numberOfRadioButtons == 1){
                    $('.superleaf .selectionButtons').addClass('singleRadioButton');
                }
                if(window.numberOfRadioButtons == 0){
                    $('.superleaf .selectionButtons').addClass('noRadioButton');
                }
            }
            setActionPanel(apClass);
        }
        actionPanelInit();
        A.trigger('actionPanelStateUpdated');
        
        P.when('superleaf-can-lazy-load-radiobuttons').execute(function (){
            A.trigger('superleafLazyLoadRadioButtons');
        });
    });
    
    P.when("accordionBuyBoxJS").execute(function(buyBoxJS){
        buyBoxJS.initialize();
    });
    
    if(window.superleaf && window.superleaf.twisterRefreshInProgress){
        window.superleaf.twisterRefreshInProgress = false;
    }
</script>

</div>










