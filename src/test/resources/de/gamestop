        function InitializeMapForCandC() {
            loadData();

            var controllerMethod;
            if (clickAndCollect) {
                controllerMethod = "CollectInStore/GetStoresForStoreLocatorBySkuCandC";
            } else {
                controllerMethod = "Checkout/GetStoresForStoreLocatorBySku";
            }

            loadAndShowStoresForReserveInStoreAsync($("#SKU").val(), controllerMethod);

            if (clickAndCollect === "True") {
                if (geolocalizationActive === "True") {
                    InizializeGeolocalization();
                }
            }
        }



        $(".radioAdd").change(function () {
            if (this.checked) {
                var type = ($(this).parent().parent().next().next().find(".variantName").text());
                TrackEventAnalyticsAll("ProductSheet", "RadioButton", type, 1);
            }
        });

        $('#btnOpenMap').on('click', function () {
            var sku = $(this).closest(".buySection").find(".radioAdd[type='radio']:checked").parent().parent().next().val();
            TrackEventAnalyticsAll("ProductSheet", "OpenMap", sku, 1);

            if($('#checkboxOne').prev().children().hasClass('cchecked'))
            {
                $('#btnOpenMap').attr('href', '/StoreLocator/Index?productId='+ 51840 +'&skutype=2');
            }
            else if(1 === 1)
            {
                $('#btnOpenMap').attr('href', '/StoreLocator/Index?productId='+ 51840 + '&variantType=' + 'New');
            }
            else {
                $('#btnOpenMap').attr('href', '/StoreLocator/Index?productId='+ 51840);
            }
        });

