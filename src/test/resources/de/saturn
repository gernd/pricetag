                    e.preventDefault();
                    // load addThis
                    $.getScript('//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-522daaf43c99f97c');
                    // remove inactive img
                    $(this).hide();
                });
            }
        }
        // CALL INIT
        $(document).ready(function() {
            _private.init();
        });
        // PUBLIC FUNCTIONS
        return {}
    }(jQuery));
</script>
