    window.coma.share = (function ($, undefined) {
        // PRIVATE FUNCTIONS
        var _private = {
            init: function() {
                /*$('.price-sidebar').append($('#coma_social_share'));
                $('#coma_social_share').show().css({'position':'absolute', 'bottom': -($('.coma-cart-button-info').length ? 105 : 35)});*/
