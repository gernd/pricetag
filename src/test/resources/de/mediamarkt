            var referrerRegEx = new RegExp('(' + referrerMask + ')', 'g');
            var showBacklink = (fullReferrer.match(referrerRegEx) ? true : false);
            var showBacklinkMdz = (window.location.href.indexOf('showBacklinkMdZ') !== -1 ? true : false);
            if (showBacklink || showBacklinkMdz) {
                showCampaignBacklink('javascript:history.back()');
            }
            function showCampaignBacklink(data_href){
                jQuery('#product-wrapper.cf aside#product-sidebar.stickable .contentspot').css('padding-top','0px');
                jQuery('.campaign-backlink').attr('href', data_href).css('display','inline-block');
            }
        });
    }
</script>

    </div>
</div>


